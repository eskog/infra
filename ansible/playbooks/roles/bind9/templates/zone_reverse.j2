$TTL 86400
@   IN  SOA  ns1.{{ domain_name }}. admin.{{ domain_name }}. (
        2025060801 ; Serial
        3600       ; Refresh
        1800       ; Retry
        604800     ; Expire
        86400 )    ; Minimum TTL
;
@       IN  NS      ns1.{{ domain_name }}.
{{ dns_server_ip.split('.')[-1] }}      IN  PTR     ns1.{{ domain_name }}.


